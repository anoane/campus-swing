<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="modello_di_dominio.Facolta" table="Facolta" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="universita" cascade="lock" column="UniversitaID" class="modello_di_dominio.Universita" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Iscritto" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FacoltaID" not-null="true"/>
			<one-to-many class="modello_di_dominio.Utente"/>
		</set>
		<set name="ORM_Corso" table="Corso_Facolta" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="FacoltaID" not-null="true"/>
			<many-to-many column="CorsoID" class="modello_di_dominio.Corso">
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>
