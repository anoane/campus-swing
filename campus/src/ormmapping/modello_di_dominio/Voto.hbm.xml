<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="modello_di_dominio.Voto" table="Voto" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="valore" column="Valore" type="integer" length="11" not-null="true" lazy="false"/>
		<many-to-one name="ORM_Documento" cascade="lock" column="DocumentoID" class="modello_di_dominio.Documento" not-null="true" lazy="false">
		</many-to-one>
		<many-to-one name="ORM_Utente" cascade="lock" column="UtenteID" class="modello_di_dominio.Utente" not-null="true" lazy="false">
		</many-to-one>
	</class>
</hibernate-mapping>
